use self::bingles::three;
use self::bingles::Test;
use self::bingles::printf;

mod bingles {
    #(no-mangle, test)
    fn printf(str fmt, ...) -> i32;

    fn three() -> i32 {
        printf("three: %d\n", 3);
        return 3;
    }

    struct Test {
        i32 a,
        i32 b,
        i32 c
    }
}



fn main() -> i32 {
    let Test a = Test{
        a: 1,
        b: 2,
        c: three(),
    };
    printf("%d\n", a.c);
    return 0;
}



